% postgresql, services

# Start/Stop/Status PostgreSQL
run <ACTION_CMD> sudo -iu postgres pg_ctl <ETAT> -D /data/postgresql

$ ACTION_CMD: echo "exec,view" | tr ',' '\n'

% postgresql, connexion

